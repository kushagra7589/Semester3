ORG 0000H
MAIN:
	CLR PSW.3; BANK 0 IS USED
	MOV R0,#10101010B
	MOV P0, R0

		MOV R3, #5H
LC:		MOV R0, #0FFH;
LB:		MOV R1, #0FFH;
LA:		NOP;
		DJNZ R1, LA;
		DJNZ R0, LB;
		DJNZ R3, LC;

	SETB PSW.3
	MOV R0,#01010101B; BANK 1 IS USED
	MOV P0, R0

		MOV R3, #5H
LC1:	MOV R0, #0FFH;
LB1:	MOV R1, #0FFH;
LA1:	NOP;
		DJNZ R1, LA1;
		DJNZ R0, LB1;
		DJNZ R3, LC1;

	SJMP MAIN
END
